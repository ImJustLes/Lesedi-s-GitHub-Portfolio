<Window x:Class="PROG6212POEPART1.startWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PROG6212POEPART1"
        mc:Ignorable="d" Loaded="Window_Loaded"
        Title="startWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="7*"/>
            <ColumnDefinition Width="25*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0" Background="AntiqueWhite">
            <Button x:Name="btnRegister" Content="REGISTER" Click="btnRegister_Click"/>
            <Button x:Name="btnCloseWindow" Content="" IsEnabled="False" Height="32"/>
            <Button x:Name="btnLogin" Content="LOGIN" Click="btnLogin_Click"/>
            <Button x:Name="btnCloseWindow_Copy" Content="" IsEnabled="False" Height="32"/>
            <Button x:Name="btnExit" Content="EXIT" Click="btnExit_Click"/>
        </StackPanel>
        <Frame Name="frmRegisterLogin" Grid.Column="1"/>
    </Grid>
</Window>
